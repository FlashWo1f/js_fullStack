page,body{
    width: 100%;
}
.whole{
    position: relative;
    display: flex;
    padding: 0 20px 20px 20px;
}
.head-battle{
    width: 375px;
    text-align: center;
}
.info{
    display: inline-block;
    border-bottom: 1px solid #000000;
    flex:1;
    padding-left: 20px;
    line-height: 1;
}
.date{
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    right: 20px;
    line-height: 1;
}
